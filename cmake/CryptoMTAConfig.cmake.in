include(CMakeFindDependencyMacro)
@CryptoMTA_FIND_DEPENDENCY_CALLS@
include("${CMAKE_CURRENT_LIST_DIR}/@exported_targets_filename@")
message(STATUS "CryptoMTA_DIR = ${CryptoMTA_DIR}")
message(STATUS "CMAKE_INSTALL_PREFIX = ${CMAKE_INSTALL_PREFIX}")

@PACKAGE_INIT@
set_and_check(CryptoMTA_INCLUDE_DIR  "${CMAKE_INSTALL_PREFIX}/include/safeheron")
set_and_check(CryptoMTA_INCLUDE_DIRS "${CMAKE_INSTALL_PREFIX}/include/safeheron")
set_and_check(CryptoMTA_LIBRARY_DIR "${CMAKE_INSTALL_PREFIX}/lib")


MESSAGE(STATUS "operation system is ${CMAKE_SYSTEM}")

IF (CMAKE_SYSTEM_NAME MATCHES "Darwin")
    MESSAGE(STATUS "current platform: Darwin ")
    set_and_check(CryptoMTA_LIBRARY "${CMAKE_INSTALL_PREFIX}/lib/libCryptoMTA.dylib")
ELSEIF (CMAKE_SYSTEM_NAME MATCHES "Windows")
    MESSAGE(STATUS "current platform: Windows")
    set_and_check(CryptoMTA_LIBRARY "${CMAKE_INSTALL_PREFIX}/lib/libCryptoMTA.dll")
ELSE ()
    MESSAGE(STATUS "other platform: ${CMAKE_SYSTEM_NAME}")
    set_and_check(CryptoMTA_LIBRARY "${CMAKE_INSTALL_PREFIX}/lib/libCryptoMTA.so")
ENDIF (CMAKE_SYSTEM_NAME MATCHES "Darwin")

MESSAGE(STATUS "###################################")
